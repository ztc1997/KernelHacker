<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InfoProgressBar">
        <attr name="ipb_maxValue" format="integer"/>
        <attr name="ipb_minValue" format="integer"/>
        <attr name="ipb_unit" format="string"/>
        <attr name="ipb_name" format="string"/>
        <attr name="ipb_defaultValue" format="integer"/>
        <attr name="ipb_barColor" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="PreferenceView">
        <attr name="pv_key" format="string"/>
        <attr name="pv_title" format="string"/>
        <attr name="pv_summary" format="string"/>
    </declare-styleable>
    
    <declare-styleable name="PreferenceSwitchView">
        <attr name="psv_defaultValue" format="boolean"/>
    </declare-styleable>
</resources>